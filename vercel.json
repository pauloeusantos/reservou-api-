{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(usuarios|restaurantes|reservas|feedbacks|api/login)(.*)",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "/public/home.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}